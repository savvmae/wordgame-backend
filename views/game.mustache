       <h1>The Word contains: </h1>{{#correctGuesses}} <p>{{.}}</p> {{/correctGuesses}}
       <h1>Incorrect Guesses</h1>{{#incorrectGuesses}} <p> {{.}} </p> {{/incorrectGuesses}}
       <p>{{newWordArr}}</p>

    </body>
</html>