    <body>
       <p> {{newWord}}</p>
      

        <form method="POST" action="/game">
            <input type="text" name="guess" autofocus>
            <button type="submit">Guess</button>
        </form>
        <p>{{#currentGuess}} You guessed: {{.}} {{/currentGuess}}</p>
        <p>Incorrect Guess Count: {{guessCount}}</p>
